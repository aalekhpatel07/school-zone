{
  "expo": {
    "name": "SchoolZone",
    "slug": "SchoolZone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#00BFFF",
          "sounds": ["./assets/sounds/car_crash.wav"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "This app needs to know where you are to show the nearest school zones."
      },
      "bundleIdentifier": "com.SchoolZone"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.SchoolZone"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
